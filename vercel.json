{
    "version": 2,
    "builds": [
      {
        "src": "dist/*.html",
        "use": "@vercel/static-build"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/dist/(.*)",
        "dest": "/dist/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }
  