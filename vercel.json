{
    "version": 2,
    "builds": [
        {
          "src": "/src/app/(.*)",
          "use": "@vercel/static-build"
        },
        {
          "src": "public/**/*",
          "use": "@vercel/static"
        }
      ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/src/app/(.*)",
        "dest": "/src/app/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/src/app/$1"
      }
    ]
  }
  